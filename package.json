    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "eslint": "^8.23.1",
    "npm-run-all": "^4.1.5",
    "wrangler": "^2.0.27"
  },
  "engines": {
    "node": ">=16.13"
